<h1>{{this.title}}</h1>
<div class="project-card-list">
  {{#each this.list}}
    <div class="project-card">
      <a
        href="https://github.com/IvanS1991/{{this.name}}"
        class="project-card__link"
        target="tab"
      ><h2>{{this.name}}</h2></a>
      <p class="project-card__description">{{this.description}}</p>
      <p class="project-card__main-language">{{this.mainLanguage}}</p>
      {{#if this.deployed}}
        <a href="{{this.deployed}}" class="project-card__deployed"><h2
          >Deployed</h2></a>
      {{/if}}
    </div>
  {{/each}}
</div>