<h1>Some project I've worked on</h1>
<div class="project-card-list">
  {{#each this.en.list}}
  <div class="project-card">
    <a href="{{this.url}}" class="project-card__link"><h2>{{this.name}}</h2></a>
    <p class="project-card__description">{{this.description}}</p>
    <p class="project-card__main-language">{{this.mainLanguage}}</p>
  </div>
  {{/each}}
</div>